<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bdth.traceplus.mapper.AccountMapper">

    <!-- 开启二级缓存 -->
    <!--<cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>-->

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bdth.traceplus.domain.Account">
        <id column="account_id" property="accountId" />
        <result column="account_name" property="accountName" />
        <result column="account_phone" property="accountPhone" />
        <result column="account_password" property="accountPassword" />
        <result column="account_role" property="accountRole" />
        <result column="account_manuf_id" property="accountManufId" />
        <result column="account_realName" property="accountRealname" />
        <result column="account_idNumber" property="accountIdnumber" />
        <result column="account_img" property="accountImg" />
        <result column="account_remark" property="accountRemark" />
        <result column="create_by" property="createBy" />
        <result column="create_date" property="createDate" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        account_id, account_name, account_phone, account_password, account_role, account_manuf_id, account_realName, account_idNumber, account_img, account_remark, create_by, create_date, status
    </sql>

</mapper>
